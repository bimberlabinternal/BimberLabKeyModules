<customView xmlns="http://labkey.org/data/xml/queryCustomView" label="All Requests" canOverride="false">
    <columns>
        <column name="rowid"/>
        <column name="lastname"/>
        <column name="firstname"/>
        <column name="createdby/email"/>
        <column name="title"/>
        <column name="institutionname"/>
        <column name="status"/>
