<customView xmlns="http://labkey.org/data/xml/queryCustomView" label="MCC Requests" canOverride="false">
    <columns>
        <column name="requestId/rowid"/>
        <column name="requestId/lastname"/>
        <column name="requestId/firstname"/>
        <column name="requestId/institutionname"/>
        <column name="requestId/status"/>
        <column name="preliminaryScore"/>
        <column name="resourceAvailabilityScore"/>
        <column name="researchAreaPriorityScore"/>
        <column name="proposalScore"/>

        <column name="actions"/>
    </columns>
    <sorts>
        <sort column="requestId/rowid" descending="true"/>
    </sorts>
</customView>