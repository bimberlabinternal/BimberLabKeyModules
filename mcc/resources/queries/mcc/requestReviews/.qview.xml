<customView xmlns="http://labkey.org/data/xml/queryCustomView" label="All Reviews" canOverride="false">
    <columns>
        <column name="requestId/rowid"/>
        <column name="requestId/lastname"/>
        <column name="requestId/firstname"/>
        <column name="requestId/status">
            <properties>
                <property name="columnTitle" value="Request Status"/>
            </properties>
        </column>
        <column name="review"/>
        <column name="actions"/>
    </columns>
    <sorts>
        <sort column="requestId/rowid" descending="true"/>
    </sorts>
</customView>