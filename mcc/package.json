{
  "name": "mcc",
  "version": "0.0.0",
  "scripts": {
    "setup": "npm ci",
    "build": "npm run build-dev",
    "start": "cross-env NODE_ENV=development webpack serve --config node_modules/@labkey/build/webpack/watch.config.js",
    "build-dev": "npm run clean && cross-env NODE_ENV=development webpack --config node_modules/@labkey/build/webpack/dev.config.js --color",
    "build-prod": "npm run clean && cross-env NODE_ENV=production webpack --config node_modules/@labkey/build/webpack/prod.config.js --color --progress --profile",
    "clean": "rimraf resources/web/mcc/gen && rimraf resources/web/gen",
    "test": "cross-env NODE_ENV=test jest"
  },
  "dependencies": {
    "@labkey/api": "^1.23.0",
    "@labkey/components": "~2.217.0",
    "chart.js": "^3.7.0",
    "react-chartjs-2": "^4.3.1",
    "uuid": "^9.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hot-loader": "^4.13.0",
    "react-tooltip": "^4.2.21",
    "@material-ui/core": "^4.12.4",
    "@material-ui/styles": "^4.11.5",
    "@mui/x-data-grid": "^4.0.2",
    "tsv": "^0.2.0"
  },
  "devDependencies": {
    "@labkey/build": "^6.14.0",
    "@types/jest": "^29.0.0",
    "@types/jquery": "^3.0.0",
    "@types/tsv": "^0.2.1",
    "@types/chart.js": "^2.9.37",
    "@types/jexl": "^2.3.1",
    "@types/node": "^18.17.1",
    "enzyme": "^3.11.0",
    "jest": "^29.0.0",
    "jest-cli": "^29.0.0",
    "jest-mock": "^29.0.0",
    "ts-jest": "^29.0.0",
    "rimraf": "^3.0.2",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.8.0",
    "jsdom-global": "^3.0.2"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsconfig": "node_modules/@labkey/build/webpack/tsconfig.json"
      },
      "LABKEY": {}
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>src/client/test/jest.setup.ts"
    ],
    "moduleNameMapper": {
      "\\.(css)$": "<rootDir>/src/client/test/styleMock.ts"
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
